datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum ContainerType {
  Plot
  Bag
}

model Container {
  uuid       String        @id @default(cuid())
  dirthDepth Float
  type       ContainerType

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
