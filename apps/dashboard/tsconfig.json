{
  "extends": "@gardentify/tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/modules/containers/components/user-container/management/delete/user-container-management-delete.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@contexts/*": ["contexts/*"],
      "@hooks/*": ["hooks/*"],
      "@lib/*": ["lib/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@typedefs/*": ["typedefs/*"],
      "@state/*": ["state/*"],
      "@public/*": ["..public/*"],
      "@typings/*": ["typings/*"],
      "@modules/*": ["modules/*"]
    }
  }
}
