{
  "name": "@gardentify/ui",
  "version": "0.0.1",
  "private": true,
  "license": "GPL-3.0",
  "author": {
    "name": "Faustino Zanetto",
    "email": "zanettofaustino@gmail.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react && tailwindcss -i ./src/styles/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup src/index.ts --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "@gardentify/eslint-config-custom": "*",
    "@gardentify/tailwind-config": "*",
    "@gardentify/tsconfig": "*",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5"
  }
}
